extends: airbnb/base
env:
  node: true,
  amd: true,
  mocha: true
globals:
  google: true
rules:
  camelcase: 0
  comma-dangle: 0
  func-names: 0
  global-require: 0
  new-cap: 0
  no-param-reassign: 0
  no-path-concat: 1
  no-underscore-dangle: 0
  no-unused-vars: 0
  object-shorthand: 0
  prefer-arrow-callback:
    - 2
    -
      allowNamedFunctions: true
  prefer-template: 0
  strict: 0
  valid-jsdoc: 1
